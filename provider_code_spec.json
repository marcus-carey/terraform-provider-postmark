							"default": {
								"static": false
							}
						}
					},
					{
						"name": "raw_email_enabled",
						"bool": {
							"computed_optional_required": "computed_optional",
							"description": "When enabled, the raw email content will be included with inbound webhook payloads under the RawEmail key.",
							"default": {
								"static": false
							}
						}
					},
