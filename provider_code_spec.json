						"string": {
							"computed_optional_required": "computed_optional",
							"description": "Color of the server in the server list, for quick identification. Possible values - 'purple', 'blue', 'turquoise', 'green', 'red', 'yellow', 'grey', 'orange'",
							"validators": [
								{
									"custom": {
										"imports": [
